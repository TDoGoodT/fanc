int multiply(int a, int bb) {
    return a * bb;
}
int decrement(int a) {
    return a - 1;
}
int increment(int a) {
    return a + 1;
}
int subtract(int a, int ab) {
    int tmpa = a;
    int tmpb = ab;
    while (tmpb > 0) {
        tmpa = decrement(tmpa);
        tmpb = decrement(tmpb);
    }
    return tmpa;
}
int modulo(int a, int ab) {
    int tmpa = a;
    while (tmpa >= ab) {
        tmpa = subtract(tmpa, ab);
    }
    return tmpa;
}
int gcd(int a, int c) {
    int tmpa = a;
    int tmpc = c;
    while (tmpa != tmpc) {
        if (tmpa > tmpc) {
            tmpa = subtract(tmpa, tmpc);
        } else {
            tmpc = subtract(tmpc, tmpa);
        }
    }
    return tmpa;

}

void main() {
    int num1 = 15;
    int num2 = 20;

    int result = gcd(num1, num2);

    print("GCD of 15 and 20: ");
    printi(result);
}
