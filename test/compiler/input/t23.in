bool getTrue() {
    return true;
}
bool getFalse() {
    return false;
}

void main(){
    bool shouldBeTrue = getTrue() or getFalse();
    bool shouldBeFalse = getTrue() and getFalse();
    if (shouldBeTrue) {
        print("We are ok");
    } else {
        if (shouldBeFalse) {
            print("error");
        }
    }
}