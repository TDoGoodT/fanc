int increment(int a) {
    return a + 1;
}
int decrement(int a) {
    return a - 1;
}
int subtract(int a, int ab) {
    int tmpa = a;
    int tmpb = ab;
    while (tmpb > 0) {
        tmpa = decrement(tmpa);
        tmpb = decrement(tmpb);
    }
    return tmpa;
}
int divide(int a, int ab) {
    int quotient = 0;
    int tmpa = a;
    while (tmpa >= ab) {
        tmpa = subtract(tmpa, ab);
        quotient = increment(quotient);
    }
    return quotient;
}

void main() {
    int num1 = 10;
    int num2 = 2;

    int result = divide(num1, num2);

    print("Division of 10 by 2: ");
    printi(result);
}
